<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  <title>Feedback</title>
</head>
<body>
  <nav class="navbar navbar-expand-sm bg-dark navbar-dark">
    <div class="container-fluid">
      <a class="navbar-brand" href="/">Airplane Ticket App</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#collapsibleNavbar">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="collapsibleNavbar">
        <ul class="navbar-nav me-auto">
          <li class="nav-item">
            <a class="nav-link" href="/addTicket">Add New Ticket</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/contact">Contact Us</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/feedback">Feedback</a>
          </li>
        </ul>
        <form class="d-flex" action="/search" method="GET">
          <input class="form-control me-2" type="search" placeholder="Search Tickets" aria-label="Search" name="query">
          <button class="btn btn-outline-light" type="submit">Search</button>
        </form>
      </div>
    </div>
  </nav>
  <div class="container">
    <br>
    <div class="text-center">
      <h2>Feedback from Airplane Ticket App Database</h2>
    </div>
    <br>
    <table class="table table-hover small text-center">
      <thead>
        <tr>
          <th width="30">Name</th>
          <th width="30">Email</th>
          <th width="30">Contact No</th>
          <th width="30">Comments</th>
        </tr>
      </thead>
      <tbody>
        <% for(let i=0; i < feedback.length; i++) { %>
          <tr>
            <td><%= feedback[i].name %></td>
            <td><%= feedback[i].email %></td>
            <td><%= feedback[i].contact_no %></td>
            <td><%= feedback[i].comments %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
</body>
</html>

